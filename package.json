{
  "name": "ultimate-react-boilerplate",
  "version": "1.0.1",
  "description": "Easy way to start a project",
  "main": "prodServer.js",
  "author": "Igor Vukelic",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./src/dist && rm -rf ./src/build",
    "build-client": "cross-env NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "build-server": "cross-env NODE_ENV=production webpack -p --config webpack.config.server.prod.js",
    "start": "cross-env NODE_ENV=production node ./src/build/bundle.js",
    "start-run": "yarn-or-npm run clean && yarn-or-npm run build-client && yarn-or-npm run build-server && yarn-or-npm run start",
    "webpack-server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js",
    "webpack": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "flow": "node_modules/.bin/flow check",
    "test": "cross-env NODE_ENV=test babel-tape-runner -r ./test/setup.js ./test/**/*.test.js | node_modules\\.bin\\tap-spec",
    "coverage": "cross-env NODE_ENV=test babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/babel-tape-runner/bin/babel-tape-runner -- ./test/setup.js -- ./test/**/*.test.js"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "^2.8.0",
    "babel-runtime": "^6.23.0",
    "cross-env": "^5.0.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "3.0.4",
    "yarn-or-npm": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "bootstrap": "v4.0.0-alpha.6",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.48.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "jquery": "^3.2.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.1",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.2.1",
    "script-loader": "^0.7.0",
    "sinon": "^2.3.4",
    "sinon-test": "^1.0.2",
    "start-server-webpack-plugin": "^2.2.0",
    "style-loader": "^0.18.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tether": "^1.4.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-assets-manifest": "^0.7.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-md5-hash": "^0.0.5",
    "webpack-node-externals": "^1.6.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "repository": "git@github.com:Igor-Vuk/ultimate-react-boilerplate.git"
}
