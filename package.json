{
  "name": "ultimate-react-boilerplate",
  "version": "1.0.3",
  "description": "Easy way to start a project",
  "main": "index.js",
  "author": "Igor Vukelic",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./src/dist && rm -rf ./src/build",
    "build-server": "cross-env NODE_ENV=production babel ./src/server -d ./src/build -s --copy-files",
    "build-client": "cross-env NODE_ENV=production webpack -p --config webpack.config.prod.babel.js",
    "start": "cross-env NODE_ENV=production node ./src/build/index.js",
    "start-run": "yarn-or-npm run clean && yarn-or-npm run build-server && yarn-or-npm run build-client && yarn-or-npm run start",
    "start-dev": "cross-env NODE_ENV=development nodemon ./start-dev.js",
    "flow": "node_modules/.bin/flow check",
    "standard": "node_modules/.bin/standard --verbose | node_modules\\.bin\\snazzy",
    "test": "cross-env NODE_ENV=test babel-tape-runner -r ./test/setup.js ./test/**/*.test.js | node_modules\\.bin\\tap-spec",
    "coverage": "cross-env NODE_ENV=test babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/babel-tape-runner/bin/babel-tape-runner -- ./test/setup.js -- ./test/**/*.test.js"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "^2.7.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^5.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "3.0.4",
    "yarn-or-npm": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "bootstrap": "v4.0.0-alpha.6",
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.3",
    "enzyme": "^2.8.2",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.47.0",
    "ignore-styles": "^5.0.1",
    "jquery": "^3.2.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^2.0.5",
    "postcss-scss": "^1.0.0",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.5",
    "sass-resources-loader": "^1.2.1",
    "script-loader": "^0.7.0",
    "sinon": "^2.3.2",
    "sinon-test": "^1.0.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "style-loader": "^0.18.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tether": "^1.4.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "repository": "git@github.com:Igor-Vuk/ultimate-react-boilerplate.git"
}
