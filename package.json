{
  "name": "ultimate-react-boilerplate",
  "version": "1.0.0",
  "description": "Easy way to start a project",
  "main": "index.js",
  "author": "Igor Vukelic",
  "license": "MIT",
  "scripts": {
    "clean-client": "rm -rf ./src/dist",
    "clean-server": "rm -rf ./src/build",
    "build-server": "babel src/server -d src/build -s --copy-files",
    "build-client": "webpack -p --config webpack.config.prod.babel.js",
    "start": "set \"NODE_ENV=production\" && node ./src/build/index.js",
    "start-dev": "set \"NODE_ENV=development\" && nodemon ./start-dev.js",
    "webpack": "webpack -w --config webpack.config.babel.js",
    "flow": "./node_modules/.bin/flow check",
    "standard": "node_modules/.bin/standard --verbose | node_modules/.bin/snazzy"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "^2.6.0",
    "babel-runtime": "^6.23.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "postcss-scss": "^0.4.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "bootstrap": "v4.0.0-alpha.6",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.28.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.43.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "sass-resources-loader": "^1.2.1",
    "script-loader": "^0.7.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1",
    "style-loader": "^0.16.1",
    "tether": "^1.4.0",
    "webpack": "^2.3.2"
  },
  "repository": "git@github.com:Igor-Vuk/ultimate-react-boilerplate.git"
}
